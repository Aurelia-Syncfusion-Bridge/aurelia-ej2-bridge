{
  "name": "aurelia-ej2-bridge",
  "private": false,
  "homepage": "http://logoui.co.il/en",
  "bugs": {
    "url": "https://github.com/Aurelia-Syncfusion-Bridge/aurelia-ej2-bridge.git/issues"
  },
  "license": "MIT",
  "author": "David Kossoglyad <david@logoui.co.il> (https://github.com/Aurelia-Syncfusion-Bridge/aurelia-ej2-bridge)",
  "contributors": [],
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aurelia-Syncfusion-Bridge/aurelia-ej2-bridge.git"
  },
  "scripts": {
    "lint": "cross-env tslint --project tsconfig.json",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" karma start --single-run --package=core",
    "develop": "cross-env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" karma start --package=core",
    "clean": "lerna clean",
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build  --ignore @aurelia-ej2-bridge/__e2e__",
    "predoc": "cross-env rimraf doc/api.json && rimraf dist/doc-temp && tsc --project tsconfig.json --outFile dist/doc-temp/aurelia-ej2-bridge.js && node doc/shape-defs && copyfiles tsconfig.json dist/doc-temp",
    "doc": "cross-env typedoc --json doc/api.json --excludeExternals --includeDeclarations --mode modules --target ES6 --name aurelia-ej2-bridge-docs dist/doc-temp/",
    "postdoc": "cross-env node doc/shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p angular -i doc/CHANGELOG.md -s",
    "bump-version": "npm --no-git-tag-version version",
    "preprepare-release": "cross-env npm run test",
    "prepare-release": "cross-env npm run changelog && npm run build && npm run doc"
  },
  "dependencies": {
    "aurelia-binding": "^2.5.2",
    "aurelia-dependency-injection": "^1.5.2",
    "aurelia-loader": "^1.0.2",
    "aurelia-logging": "^1.5.2",
    "aurelia-metadata": "^1.0.6",
    "aurelia-pal": "^1.8.2",
    "aurelia-task-queue": "^1.3.3",
    "aurelia-templating": "^1.10.3",
    "aurelia-templating-resources": "^1.13.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/karma": "^1.7.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/webpack": "^4.41.7",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-pal-browser": "^1.8.1",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-polyfills": "^1.3.4",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^4.0.0",
    "concurrently": "^3.4.0",
    "conventional-changelog-cli": "^2.0.31",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.2.4",
    "html-entities": "^1.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.99.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "lerna": "^3.20.2",
    "path": "^0.12.7",
    "requirejs": "^2.3.6",
    "requirejs-text": "^2.0.15",
    "rimraf": "^3.0.2",
    "strip-ansi": "^6.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-dev-server": "^3.10.3"
  }
}
